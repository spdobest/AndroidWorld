<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="spm.androidworld.all.autoaddresssearch.AutoAddressSearchActivity">

    <fragment
            android:id="@+id/headlines_fragment"
            android:name="spm.androidworld.all.edittextWIdgets.fragments.AutoAddressSearchFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#ee00"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/headlines_fragment" />

</androidx.constraintlayout.widget.ConstraintLayout>